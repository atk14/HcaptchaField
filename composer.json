{
        "name": "atk14/hcaptcha-field",
        "type": "library",
        "description": "A field for protecting forms against spam in ATK14 applications - an alternative to RecaptchaField without Google",
        "keywords": ["captcha", "hcaptcha", "field", "forms", "atk14"],
        "homepage": "https://github.com/atk14/HcaptchaField",
        "license": ["MIT"],
        "authors": [
                {
                        "name": "ATK14 Development Team",
                        "homepage": "https://github.com/orgs/atk14/people"
                }
        ],
        "require": {
                "php": ">=5.6.0",
                "atk14/url-fetcher": "^1.7"
        },
        "require-dev": {
                "atk14/tester": "0.3.*",
                "atk14/forms": "dev-master",
                "atk14/core": "dev-master",
                "atk14/http": "^0.8"
        },
        "autoload": {
                "classmap": ["src/app/fields","src/app/widgets"]
        }
}
